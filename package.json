{
    "name": "attendance-management",
    "version": "1.0.0",
    "scripts": {
        "start": "npm run dev",
        "client": "npm.cmd run dev --prefix client",
        "server": "npm.cmd run dev --prefix server",
        "dev": "concurrently \"npm.cmd run server\" \"npm.cmd run client\"",
        "build": "cd client && npm install && npm run build"
    },
    "devDependencies": {
        "concurrently": "^8.2.2"
    }
}